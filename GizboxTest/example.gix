GC::Disable();

namespace Creature
{
    int totalCount = 0;

    Creature Born(string type)
    {
        if(type == "Human")
        {
            Console::Log("Create Human...");
            Human inst = new Human();
            Creature::totalCount ++;
            return (Creature)inst;  
        }
        else
        {
            return null;
        }
    }
    void Die(Creature c)
    {
        if(c != null)
        {
            Console::Log("Delete Creature:" + c.name);
            delete c;
            Creature::totalCount --;
        }
    }
}
class Creature
{    
    string name = "";
    int age = 0;

    void Move()
