}
class Human : Creature
{
    int job = 0;
    
    void Move()
    {
        Console::Log(this.name + " walk...");
    }
}

//Test  
Human sam = (Human)Creature::Born("Human");
sam.name = "Sam";
sam.age = 20;
sam.job = Human::Job::Student;

sam.Move();

for(int i = 0; i < 99; i++)
{
    sam.age += 1;

    if((sam.age % 10) == 0)
    {
        Console::Log(sam.name + " is now " + (string)(sam.age) + " years old!");
    }

    if(sam.age > 90)
    {
        Creature::Die(sam);
        break;
    }
}


